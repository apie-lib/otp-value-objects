{
  "name": "apie/otp-value-objects",
  "description": "Composer package of the apie library: otp value objects",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\OtpValueObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\OtpValueObjects\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "apie/core": "self.version",
    "apie/serializer": "self.version",
    "chillerlan/php-qrcode": "^5.0.2",
    "spomky-labs/otphp": "^11.2.2"
  },
  "require-dev": {
    "apie/fixtures": "self.version",
    "apie/service-provider-generator": "0.11.2",
    "phpunit/phpunit": "^9.6.19",
    "phpspec/prophecy-phpunit": "^2.2"
  },
  "conflict": {
    "thecodingmachine/safe": "1.*"
  },
  "extra": {
    "apie-objects": [
      "Apie\\OtpValueObjects\\HOTPSecret",
      "Apie\\OtpValueObjects\\OTP",
      "Apie\\OtpValueObjects\\TOTPSecret"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}